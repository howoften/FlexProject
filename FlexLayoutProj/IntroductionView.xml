<?xml version="1.0" encoding="UTF-8"?>
<UIView layout="position:relative, margin:0, padding:0, width:100%, height:100%" attr="bgColor:white">
    <FlexScrollView name="_contentView" layout="position:absolute, display:flex, flexDirection:row, margin:0, width:100%, height:100%" attr="pageEnabled:true, horzScroll:true, bgColor:#f2f2f2">
        <UIView layout="height:100%, width:ScreenWidth, display:flex, flexDirection:col, justifyContent:center, alignItems:center" attr="">
            <UIView layout="display:flex, flexDirection:row" attr="">
                <UILabel name="" layout="" attr="font:heavy|40,color:#000000,lineBreakMode:truncatingTail,linesNum:1,text:Linked"/>
                <UILabel name="" layout="marginLeft:5, padding:5/0/5/0" attr="font:heavy|40,color:#ffffff,bgColor:#4682B4, borderRadius:4, masksToBounds:true, lineBreakMode:truncatingTail,linesNum:1, textAlign:center, text:in"/>
            </UIView>
            <UIView layout="height:adaptedVertical(70)" attr="">
            </UIView>
        </UIView>
        <UIView layout="height:100%, width:ScreenWidth, display:flex, flexDirection:col, justifyContent:flex-start, alignItems:center" attr="bgColor:#4682B4">
            <UILabel name="" layout="padding:0/statusBarAddedTo(40)/0/0, width:100%" attr="font:medium|32,color:#ffffff, textAlign:center, linesNum:0,text:创建职业档案，拓展职\n场机遇"/>
            <UIView layout="position:relative, width:100%" attr="bgColor:black">
                <UIView layout="position:absolute, width:80%, display:flex, flexDirection:col justifyContent:center, alignSelf:center, top:adaptedHorizen(35), paddingTop:adaptedHorizen(60)" attr="bgColor:white">
                    <UILabel name="" layout="marginTop:5" attr="font:26,color:#333333,lineBreakMode:truncatingTail,linesNum:1, textAlign:center, text:陈嘉莉"/>
                    <UILabel name="" layout="marginTop:5" attr="font:17,color:#333333,lineBreakMode:truncatingTail,linesNum:1, textAlign:center, text:市场经理"/>
                    <UILabel name="" layout="marginTop:5" attr="font:15,color:#333333,lineBreakMode:truncatingTail,linesNum:1, textAlign:center, text:上海浦东"/>
                    <UIView layout="width:100%, height:1, marginTop:5" attr="bgColor:#666666"/>
                    <FlexTouchView onPress="" layout="height:40, width:100%" attr="underlayColor:darkGray">
                        <UILabel name="" layout="alignSelf:center, height:100%" attr="fontSize:17,color:#4682B4,lineBreakMode:truncatingTail,linesNum:1,text:加为好友"/>
                    </FlexTouchView>
                </UIView>
                <UIView layout="position:absolute, display:flex,flexDirection:row, alignSelf:center, top:0, width:adaptedHorizen(90),height:adaptedHorizen(90)" attr="bgColor:white, masksToBounds:true, borderRadius:calculate(adaptedHorizen(90)/2)">
                    <UIImageView layout="flex:1, margin:5/5/5/5" attr="masksToBounds:true, borderRadius:calculate(adaptedHorizen(80)/2), source:abc, bgColor:gray"/>
                </UIView>
            </UIView>
        </UIView>
        <UIView layout="height:100%, width:ScreenWidth, display:flex, flexDirection:col, justifyContent:flex-start, alignItems:center" attr="bgColor:#4682B4">
            <UILabel name="" layout="padding:0/statusBarAddedTo(40)/0/0, width:100%" attr="font:medium|32,color:#ffffff, textAlign:center, linesNum:0,text:拓展职场人脉，时\n常保持联系"/>
            <UIView layout="display:flex, flexDirection:row, justifyContent:flex-start, alignItems:center" attr="">
                <UIImageView layout="width:adaptedHorizen(50), height:adaptedHorizen(50)" attr="source:abv, bgColor:lightGray, borderRadius:calculate(adaptedHorizen(50)/2), clipsToBounds:true"/>
                <UILabel name="" layout="marginLeft:11, padding:8" attr="font:medium|16,color:#000000,lineBreakMode:truncatingTail,linesNum:1, textAlign:center, bgColor:white, text:恭喜你升职了～ 一起喝杯咖啡？"/>
            </UIView>
            <UIView layout="height:60" attr=""/>
            <UIView layout="display:flex, flexDirection:row, justifyContent:flex-end, alignItems:center, width:100%" attr="">
                <UILabel name="" layout="marginRight:22, padding:8" attr="font:medium|16,color:#000000,lineBreakMode:truncatingTail,linesNum:1, textAlign:center, bgColor:#87CEFA, text:好呀，谢谢～"/>
            </UIView>
        </UIView>
        <UIView layout="height:100%, width:ScreenWidth, display:flex, flexDirection:col, justifyContent:flex-start, alignItems:center" attr="bgColor:#4682B4">
            <UILabel name="" layout="padding:0/statusBarAddedTo(40)/0/0, width:100%" attr="font:medium|32,color:#ffffff, textAlign:center, linesNum:0,text:有熟人帮忙，找工\n作更方便"/>
            <UIView layout="display:flex, flexDirection:col, justifyContent:center, alignItems:flex-start, padding:10" attr="bgColor:white">
                <UIView layout="display:flex, flexDirection:row, justifyContent:flex-start, alignItems:center" attr="">
                    <UIImageView layout="width:adaptedHorizen(50), height:adaptedHorizen(50)" attr="source:abv, bgColor:lightGray, borderRadius:calculate(adaptedHorizen(50)/2), clipsToBounds:true"/>
                    <UILabel name="" layout="marginLeft:11, padding:8" attr="font:regular|16,color:#000000,lineBreakMode:truncatingTail,linesNum:1, textAlign:center, bgColor:white, text:嘉丽，可以推荐您申请2个职位"/>
                </UIView>
                <UIView layout="height:20, width:60, display:flex, flexDirection:col, justifyContent:center, alignItems:center" attr="">
                    <UIView layout="left:0, top:0, height:1, width:100%" attr="bgColor:lightGray"/>
                </UIView>
                <UIView layout="display:flex, flexDirection:row, justifyContent:flex-start, alignItems:center" attr="">
                    <UIImageView layout="width:adaptedHorizen(35), height:adaptedHorizen(35)" attr="source:abv, bgColor:lightGray"/>
                    <UIView layout="marginLeft:6, display:flex, flexDirection:col, justifyContent:center, alignItems:flex-start" attr="">
                        <UILabel name="" layout="marginLeft:6" attr="font:medium|15,color:#000000,lineBreakMode:truncatingTail,linesNum:1, text:新职位"/>
                        <UIView layout="height:5, width:200, marginTop:5" attr="bgColor:gray"/>
                        <UIView layout="height:5, width:120, marginTop:5" attr="bgColor:gray"/>
                    </UIView>
                </UIView>
                <UIView layout="height:10" attr=""/>
                <UIView layout="display:flex, flexDirection:row, justifyContent:flex-start, alignItems:center" attr="">
                    <UIImageView layout="width:adaptedHorizen(35), height:adaptedHorizen(35)" attr="source:abv, bgColor:lightGray"/>
                    <UIView layout="marginLeft:6, display:flex, flexDirection:col, justifyContent:center, alignItems:flex-start" attr="">
                        <UILabel name="" layout="marginLeft:6" attr="font:medium|15,color:#000000,lineBreakMode:truncatingTail,linesNum:1, text:新的工作机会"/>
                        <UIView layout="height:5, width:200, marginTop:5" attr="bgColor:gray"/>
                        <UIView layout="height:5, width:120, marginTop:5" attr="bgColor:gray"/>
                    </UIView>
                </UIView>
            </UIView>

        </UIView>
    </FlexScrollView>

    <UIPageControl name="" layout="position:absolute, alignSelf:center, marginTop:calculate(statusBarAddedTo(0)+adaptedVertical(520))" attr="numberOfPages:4, currentPage:0, pageTintColor:#666666, curPageTintColor:#000000"/>
    <UIView layout="position:absolute, display:flex, flexDirection:row, justifyContent:center, alignItems:center width:100%, height:adaptedHorizen(45), marginTop:calculate(statusBarAddedTo(0)+adaptedVertical(560))" attr="">
        <UIView layout="display:flex, flexDirection:row, justifyContent:flex-end, width:50%, paddingRight:7" attr="">
            <FlexTouchView onPress="" layout="width:80%, height:100%" attr="underlayColor:darkGray">
                <UILabel name="" layout="width:100%, height:100%" attr="font:bold|22,color:#ffffff, bgColor:#4682B4, lineBreakMode:truncatingTail,linesNum:1, masksToBounds:true, borderRadius:5, textAlign:center, text:立即加入"/>
            </FlexTouchView>
        </UIView>
        <UIView layout="display:flex, flexDirection:row, justifyContent:flex-start, width:50%, paddingLeft:7" attr="">
            <FlexTouchView onPress="" layout="width:80%, height:100%" attr="underlayColor:darkGray">
                <UILabel name="" layout="width:100%, height:100%" attr="font:bold|22,color:#4682B4, bgColor:#ffffff, lineBreakMode:truncatingTail,linesNum:1,masksToBounds:true, borderRadius:5, borderColor:#4682B4, borderWidth:1, textAlign:center, text:登录"/>
            </FlexTouchView>
        </UIView>
    </UIView>
</UIView>
 